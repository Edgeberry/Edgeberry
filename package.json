{
  "name": "edgeberry",
  "version": "2.4.1",
  "description": "Turn your Raspberry Pi into an IoT edge device!",
  "author": "Sanne 'SpuQ' Santens",
  "license": "MIT",
  "private": false,
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && webpack --config webpack.config.js && uglifyjs --mangle toplevel --output build/index.js -- build/bundle.js && rm -rf build/bundle.js jsbuild/  && cp -r src/public build/",
    "deploy": "bash ./deploy.sh",
    "dev": "nodemon --watch src src/index.ts",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpuQ/EdgeBerry.git"
  },
  "keywords": [
    "iot",
    "edge",
    "device",
    "raspberry",
    "pi",
    "aws",
    "azure",
    "mqtt"
  ],
  "bugs": {
    "url": "https://github.com/SpuQ/EdgeBerry/issues"
  },
  "homepage": "https://github.com/SpuQ/EdgeBerry#readme",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "babel-loader": "^9.1.3",
    "nodemon": "^3.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "uglify-js": "^3.17.4",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@spuq/json-ipc": "^1.1.0",
    "aws-iot-device-sdk-v2": "^1.19.1",
    "azure-iot-device": "^1.18.3",
    "azure-iot-device-mqtt": "^1.16.3",
    "azure-iot-provisioning-device": "^1.9.1",
    "azure-iot-provisioning-device-mqtt": "^1.8.1",
    "azure-iot-security-symmetric-key": "^1.8.2",
    "azure-iot-security-x509": "^1.8.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "pm2": "^5.3.1"
  }
}
